<odoo>
    <record id="view_hr_employee_services_report" model="ir.ui.view">
        <field name="name">view_hr_employee_services_report_tree</field>
        <field name="model">hr.employee.services.report</field>
        <field name="arch" type="xml">
            <tree >
                <field name="employee_id"/>
                <field name="partner_id"/>
                <field name="total_amount"/>
                <field name="commission_rate"/>
            </tree>
        </field>
    </record>

    <record id="action_views_hr_employee_services_report" model="ir.actions.act_window">
        <field name="name">Reportes</field>
        <field name="res_model">hr.employee.services.report</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="menu_hr_employee_services" name="Pokemon" action="action_views_hr_employee_services_report" />
</odoo>